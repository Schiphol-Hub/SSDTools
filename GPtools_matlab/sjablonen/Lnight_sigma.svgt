<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg width="140mm" height="140mm"
     <plot object="viewbox"
           id="ref" 
           size="0,0,1400,1400"/>
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">

	 
<svg <plot object="viewbox" id="plot1" size="3000,27875,60000,60000"/>>
          
	<!--  Achtergrond: A4, 300 dpi -->
	<image xlink:href="Schiphol_RD900dpi.png" x="0" y="0" width="78750" height="111375"/>

	<g transform="scale(1,-1) translate(-80000,-541375)">
		
		<!-- betrouwbaarheidsinterval 40 en 48 dB contour -->
		<plot object="contour_plot"
			id="plot1a"
			type="hilo"
			method="tape2"
			grid="dhi"
			grid2="dlo"
			dB="40,48"
			stroke="40,#e093ff,0.5;50,#9933ff,0.5"
			fill="40,#e093ff,0.5;50,#9933ff,0.5"
			style="fill-rule:evenodd;stroke-width:50"
		/>

		<!-- nominale 40 en 48 dB contour -->
		<plot object="contour_plot"
			id="plot1b"
			type="line"
			method="tape2"
			grid="mean"
			dB="40,48"
			stroke="40,#e093ff,0.5;50,#9933ff,0.5"
			fill="none"
			style="fill:none;stroke-width:150"
		/>
		<plot object="contour_plot"
			id="plot1c"
			type="line"
			method="tape2"
			grid="mean"
			dB="40,48"
			stroke="40,#ffffff,1.0;50,#ffffff,1.0"
			fill="none"
			style="fill:none;stroke-width:100"
		/>
					
	</g>
</svg>

<!-- Onderstaande staat op een vaste plaats op de pagina -->

<!-- schaalbalk -->
<g transform="translate(35,1350)">
	<plot object="scalebar"
		id="plot1"
		range="0:1000:10000"
		style1="fill:#333333;stroke:#333333;stroke-width:2"
		style2="fill:none;stroke:#333333;stroke-width:2"
	    height="10"
   		label="10 km"
		label_style="font-family:Myriad Pro;font-size:20pt;font-weight:normal;fill:#333333;text-anchor:start;"
    />
</g>

<!-- Legenda -->
<g transform="translate(1090,1265)">
    <!-- kader -->
	<rect width="275" height="100" x="0" y="0" style="fill:#ffffff;fill-opacity:0.7;stroke:#aaaaaa;stroke-width:1.0" />
	
	<!-- contouren -->
	<rect x="20" y="10" width="120" height="30" style="fill:#e093ff;fill-opacity:0.5;stroke:none"/>
    <rect x="20" y="60" width="120" height="30" style="fill:#9933ff;fill-opacity:0.5;stroke:none"/>
		
	<path style="fill:none;stroke:#ffffff;stroke-width:2.5;stroke-opacity:1"
		d="M 20,17 q 15 -5 60 8 q 45 13 60 8
		   M 20,67 q 15 -5 60 8 q 45 13 60 8"
	/>
	
	<g style="font-family:Myriad Pro;font-size:20pt;font-weight:normal;fill:#333333;text-anchor:left">
		<text x="160" y="35">40 dB(A)</text>
		<text x="160" y="85">48 dB(A)</text>
	</g>

</g>

</svg>