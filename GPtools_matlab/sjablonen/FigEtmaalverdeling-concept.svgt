<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg viewBox="0 0 800 330"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">
     
<!-- Filter voor schaduw -->     
    <filter id = "dropshadow" width = "130%" height = "130%">
        <feOffset result = "offOut" in = "SourceGraphic" dx = "2" dy = "-2"/>
        <feColorMatrix result = "matrixOut" in = "offOut" type = "matrix" values = "0.6 0 0 0 0 0 0.6 0 0 0 0 0 0.6 0 0 0 0 0 1 0"/>
        <feGaussianBlur result = "blurOut" in = "matrixOut" stdDeviation = "3"/>
        <feBlend in = "SourceGraphic" in2 = "blurOut" mode = "normal"/>
    </filter>

<!-- Grafiek -->
<g transform="translate(50 20)">
	
    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot1"
 		type="rectangle"
		width="700"
		height="145"
		fill_style="fill:#d4dde8;fill-opacity:1.0"
		frame_style="stroke:#5badd5;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot1"
		type="categories"
		values="cat"
		position="left"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="270"
		label_offset="-7"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot1"
		type="numeric"
	    range="0:100:700"
		position="top"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="fill:none;text-anchor:middle"
		label_rotation="0"
		label_offset="-7"
		line_style="stroke:none"
	/>
	
	<plot object="bar_plot1"
		id="plot1"
		type="stacked"
		values="T"
		fill="1,#566170,1;4,#c3defe,1"
		stroke="#3b6968"
		style="stroke-width:0.5"
		bar_width="30"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#000000;text-anchor:end"
		label_xoffset="[0,0; ,-2,-2; 0,0; 10,10]"
		label_yoffset="0"
	/>

<g transform="translate(0 146)">

    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot2"
 		type="rectangle"
		width="700"
		height="145"
		fill_style="fill:#e3c68e;fill-opacity:1.0"
		frame_style="stroke:#5badd5;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot2"
		type="categories"
		values="cat"
		position="left"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="270"
		label_offset="-7"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot2"
		type="numeric"
	    range="0:100:700"
		position="bottom"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_offset="12"
		label_rotation="0"
		line_style="stroke:none"
	/>
	<plot object="bar_plot2"
		id="plot2"
		type="stacked"
		values="L"
		fill="1,#566170,1;4,#c3defe,1"
		stroke="#3b6968"
		style="stroke-width:0.5"
		bar_width="30"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#000000;text-anchor:end"
		label_xoffset="[0,0; ,-2,-2; 0,0; 10,10]"
		label_yoffset="0"
	/>
</g>
	<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
	
		     Hieronder volgt het statische deel van het sjabloon
	
	     ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->

    <!-- Scheiding tussen Plot1 en Plot2 -->
    <line x1= "0.5" y1="145.5" x2="699.5" y2="145.5" style="stroke:#ffffff;stroke-width:2.0"/>
    
    <line x1= "0"   y1="144.0" x2="0"     y2="147.0" style="stroke:#5badd5;stroke-width:0.5"/>
	<line x1= "700" y1="144.0" x2="700"   y2="147.0" style="stroke:#5badd5;stroke-width:0.5"/>

    <!-- Legenda -->
    <g transform="translate(360 -18)">
        <rect x=  "0.0" y="0" width="30.0" height="10.0" style="fill:#566170;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect x= "75.0" y="0" width="30.0" height="10.0" style="fill:#7A8B9F;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect x="150.0" y="0" width="30.0" height="10.0" style="fill:#9FB4CF;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect x="225.0" y="0" width="30.0" height="10.0" style="fill:#C3DEFE;stroke:#4d4d4d;stroke-width:0.5;"/>

        <g style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:left;">
            <text x= "35" y="8">dag</text>
            <text x="110" y="8">avond</text>
            <text x="185" y="8">nacht</text>
            <text x="260" y="8">vroege ochtend</text>
        </g>
    </g>
    
    <!-- Y-labels -->
    <g transform="translate(0,290)">
        <g transform="rotate(270)">
            <g style="font-family:Myriad Pro;font-size:14pt;font-weight:normal;fill:#333333;text-anchor:middle;">
                <text x="72.5"  y="-30">landingen</text>
                <text x="217.5" y="-30">starts</text>
            </g>
            <g style="stroke:#4d4d4d;stroke-width:0.5">
                <line x1= "10" y1="-23" x2="135" y2="-23"/>
                <line x1="155" y1="-23" x2="280" y2="-23"/>
            </g>
        </g>
    </g>


</g>

</svg>

