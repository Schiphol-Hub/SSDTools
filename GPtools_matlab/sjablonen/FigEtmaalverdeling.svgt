		label_xoffset="[0,0; -4,-4; -2,-2; 8,8]"
		label_yoffset="-20"
		label_rotation="0"
	/>
</g>
	<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
	
		     Hieronder volgt het statische deel van het sjabloon
	
	     ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->

    <!-- Scheiding tussen Plot1 en Plot2 -->
    <line x1= "0.5" y1="145.5" x2="699.5" y2="145.5" style="stroke:#ffffff;stroke-width:2.0"/>
    
    <line x1= "0"   y1="144.0" x2="0"     y2="147.0" style="stroke:#4d4d4d;stroke-width:0.5"/>
	<line x1= "700" y1="144.0" x2="700"   y2="147.0" style="stroke:#4d4d4d;stroke-width:0.5"/>

    <!-- Legenda -->
    <g transform="translate(360 -20)">
        <rect x="130" y="4" width="80" height="10.0" style="fill:#da9100;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect x="210" y="4" width="50" height="10.0" style="fill:#e4af00;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect x="260" y="4" width="30" height="10.0" style="fill:#f0d373;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect x="290" y="4" width="10" height="10.0" style="fill:#fcf7e6;stroke:#4d4d4d;stroke-width:0.5;"/>

        <g style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:end;">
            <text x="208" y="0">dag</text>
            <text x="258" y="0">avond</text>
            <text x="290" y="0">nacht</text>
        </g>
		<g style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:left;">
			<text x="302" y="0">vroege</text>
			<text x="302" y="9">ochtend</text>
		</g>
    </g>
    
    <!-- Y-labels -->
    <g transform="translate(0,290)">
        <g transform="rotate(270)">
            <g style="font-family:Myriad Pro;font-size:14pt;font-weight:normal;fill:#333333;text-anchor:middle;">
                <text x="72.5"  y="-30">landingen</text>
                <text x="217.5" y="-30">starts</text>
            </g>
            <g style="stroke:#4d4d4d;stroke-width:0.5">
                <line x1= "10" y1="-23" x2="135" y2="-23"/>
                <line x1="155" y1="-23" x2="280" y2="-23"/>
            </g>
        </g>
    </g>


</g>

</svg>

