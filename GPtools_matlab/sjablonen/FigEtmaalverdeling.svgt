<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg viewBox="0 0 800 390"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">
     
<!-- Grafiek -->
<g transform="translate(50 50)">
	
    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot1"
 		type="rectangle"
		width="700"
		height="145"
		fill_style="fill:#e6edf3;fill-opacity:1.0"
		frame_style="stroke:#4d4d4d;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot1"
		type="categories"
		values="cat"
		position="left"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="270"
		label_offset="-10"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot1"
		type="numeric"
	    range="0:100:800"
		position="top"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="fill:none;text-anchor:middle"
		label_rotation="0"
		label_offset="12"
		line_style="stroke:none"
	/>
	
	<plot object="bar_plot1"
		id="plot1"
		type="stacked"
		values="T"
		fill="1,#da9100,1;2,#e4af00,1;3,#f0d373,1;4,#fcf7e6,1"
		stroke="#4d4d4d"
		style="stroke-width:0.5"
		bar_width="30"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:end"
		label_xoffset="[0,0; -4,-4; -2,-2; 8,8]"
		label_yoffset="-20"
		label_rotation="0"
	/>

<g transform="translate(0 146)">

    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot2"
 		type="rectangle"
		width="700"
		height="145"
		fill_style="fill:#f8ebc7;fill-opacity:1.0"
		frame_style="stroke:#4d4d4d;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot2"
		type="categories"
		values="cat"
		position="left"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="270"
		label_offset="-10"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot2"
		type="numeric"
	    range="0:100:800"
		position="bottom"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_offset="12"
		label_rotation="0"
		line_style="stroke:none"
	/>
	<plot object="bar_plot2"
		id="plot2"
		type="stacked"
		values="L"
		fill="1,#da9100,1;2,#e4af00,1;3,#f0d373,1;4,#fcf7e6,1"
		stroke="#4d4d4d"
		style="stroke-width:0.5"
		bar_width="30"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
