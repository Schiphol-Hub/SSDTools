<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg viewBox="0 0 800 310"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">
    
<!-- Filter voor schaduw -->     
    <filter id = "dropshadow" width = "130%" height = "130%">
        <feOffset result = "offOut" in = "SourceGraphic" dx = "2" dy = "-2"/>
        <feColorMatrix result = "matrixOut" in = "offOut" type = "matrix" values = "0.6 0 0 0 0 0 0.6 0 0 0 0 0 0.6 0 0 0 0 0 1 0"/>
        <feGaussianBlur result = "blurOut" in = "matrixOut" stdDeviation = "3"/>
        <feBlend in = "SourceGraphic" in2 = "blurOut" mode = "normal"/>
    </filter>

<!-- Grafiek -->
<g transform="translate(50 50)">

    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot1"
 		type="rectangle"
		width="700"
		height="210"
		fill_style="fill:#f8ebc7;fill-opacity:1.0"
		frame_style="stroke:#4d4d4d;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot1"
		type="categories"
		values="months"
		position="bottom"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="0"
		label_offset="12"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot1"
		type="numeric"
	    range="0:10:100"
		position="left"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix="%"
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:end"
		label_rotation="0"
		label_offset="-7"
		line_style="stroke:none"
	/>
	
	<plot object="bar_plot1"
		id="plot1"
		type="stacked"
		values="som_HGp"
		fill="1,#da9100,1;2,#e4af00,1"
		stroke="#4d4d4d"
		style="stroke-width:0.5"
		bar_width="30"
		decimals="0"
		decimal_sign=","
		label_postfix="%"
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:middle"
		label_xoffset="-17"
		label_yoffset="-8"
	/>

	<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
	
		     Hieronder volgt het statische deel van het sjabloon
	
	     ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->

    <!-- Legenda -->
	<g transform="translate(625 -40)">
        <rect x="0" y="25" width="20" height="10.0" style="fill:#da9100;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect x="0" y="0"  width="20" height="25.0" style="fill:#e4af00;stroke:#4d4d4d;stroke-width:0.5;"/>

        <g style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:start;">
            <text x="25" y="31">per maand</text>
            <text x="25" y="8">totaal</text>
        </g>
    </g>

</g>
</svg>
