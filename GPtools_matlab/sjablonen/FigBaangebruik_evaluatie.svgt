		           dit moet voor de realisatie
	
     ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->

 <g transform="translate(50 50)">

	<!-- Scheiding tussen Plot1 en Plot2 -->
    <line x1= "349.5" y1="0.5" x2="349.5" y2="289.5" style="stroke:#ffffff;stroke-width:2.0"/>
    
    <line x1= "348" y1="0"   x2="351" y2="0"   style="stroke:#5badd5;stroke-width:0.5"/>
	<line x1= "348" y1="290" x2="351" y2="290" style="stroke:#5badd5;stroke-width:0.5"/>
	
	<!-- Tekst onder de grafiek -->
    <g transform="translate(0, 290)">
		<g style="stroke:#4d4d4d;stroke-width:0.5">
			<line x1= "10" y1="22" x2="340" y2="22"/>
			<line x1="360" y1="22" x2="690" y2="22"/>
		</g>
		<g style="font-family:Myriad Pro;font-size:14pt;font-weight:normal;fill:#333333;text-anchor:middle;">
