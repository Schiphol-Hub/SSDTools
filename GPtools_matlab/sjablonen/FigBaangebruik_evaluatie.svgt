	<plot object="plot_area"
		id="plot1"
 		type="rectangle"
		width="349"
		height="290"
		fill_style="fill:#e6edf3;fill-opacity:1.0"
		frame_style="stroke:#5badd5;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot1"
		type="categories"
		values="T_runway"
		position="bottom"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
