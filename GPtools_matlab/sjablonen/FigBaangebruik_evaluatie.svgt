        <rect id="spreiding"  x="10"  y="-18" width="10.0" height="30.0" style="fill:#5b9397;stroke:#3b6968;stroke-width:0.5;"/>
        <rect id="gemiddelde" x="0"   y="-6"  width="30.0" height="5.0"  style="fill:#adc5e2;stroke:#3b6968;stroke-width:0.5;"/>

