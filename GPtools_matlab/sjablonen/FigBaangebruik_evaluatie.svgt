		label_xoffset="0"
		label_yoffset="-2"
	/>
	
	<plot object="xy_plot1"
		id="plot1"
		type="line"
		x_values=""
		y_values="T_mean"
		style="stroke:#ffffff;stroke-opacity:0;stroke-width:2"
		marker="rechthoek"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="8"
		label_yoffset="-5"
	/>

