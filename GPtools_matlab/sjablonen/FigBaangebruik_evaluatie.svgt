     xml:space="preserve">

<defs>
    <marker id="rechthoek"
      viewBox="0 0 30 5" refX="15" refY="2.5" 
      markerUnits="userSpaceOnUse"
      markerWidth="30" markerHeight="5"
      orient="0">
      <rect width="30" height="5"
        fill="#adc5e2" stroke="#3b6968" stroke-width="0.5"  />
    </marker>

    <marker id="pijl"
      viewBox="0 0 150 150" refX="6" refY="25" 
      markerUnits="userSpaceOnUse"
      markerWidth="50" markerHeight="50"
      orient="0">
	  <path style="fill-rule:evenodd;stroke-width:1;stroke:#088A08;fill:#04B404;stroke-opacity:1.0;fill-opacity:1.0"
			d="m 15,25 c 30,-10 50,-25 50,-25 l -5,15 65,0 0,20 -65,0 5,15 c 0,0 -20,-15 -50,-25 z" />
	  <circle cx="6" cy="25" r="5"
			fill="#04B404" stroke="#088A08" stroke-width="1.0" />
    </marker>	
</defs>

<!-- .................................................................................................... -->     
<!--     Prognose                                                                                         -->
<!-- .................................................................................................... -->
     
<g transform="translate(50 50)">
	
