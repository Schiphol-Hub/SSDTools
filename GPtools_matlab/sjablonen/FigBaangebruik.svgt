<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg viewBox="0 0 800 400"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">

<defs>
    <marker id="rechthoek"
      viewBox="0 0 30 5" refX="15" refY="2.5" 
      markerUnits="userSpaceOnUse"
      markerWidth="30" markerHeight="5"
      orient="0">
      <rect width="30" height="5"
        fill="#da9100" stroke="#4d4d4d" stroke-width="1.0"  />
    </marker>
</defs>
     
<!-- Grafiek -->
<g transform="translate(50 50)">
	
    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot1"
 		type="rectangle"
		width="349"
		height="290"
		fill_style="fill:#e6edf3;fill-opacity:1.0"
		frame_style="stroke:#4d4d4d;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot1"
		type="categories"
		values="T_runway"
		position="bottom"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="0"
		label_offset="12"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot1"
		type="numeric"
	    range="0:10000:110000"
		position="left"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:end"
		label_rotation="0"
		label_offset="-7"
		line_style="stroke:none"
	/>
	
	<plot object="bar_plot1"
		id="plot1"
		type="hilo"
		values="T_min"
		values2="T_max"
		fill="#f0d373"
		stroke="#4d4d4d"
		style="stroke-width:0.5"
		bar_width="10"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="0"
		label_yoffset="-2"
	/>
	
	<plot object="xy_plot1"
		id="plot1"
		type="line"
		x_values=""
		y_values="T_mean"
		style="fill:none;stroke:#ffffff;stroke-opacity:0"
		marker="rechthoek"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="8"
		label_yoffset="-5"
	/>

	<g transform="translate(350 0)">

	<plot object="plot_area"
		id="plot2"
 		type="rectangle"
		width="349"
		height="290"
		fill_style="fill:#f8ebc7;fill-opacity:1.0"
		frame_style="stroke:#4d4d4d;stroke-width:0.5"
	/>
	
	<plot object="x_axis"
		id="plot2"
		type="categories"
		values="L_runway"
		position="bottom"
		grid_style="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="0"
		label_offset="12"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot2"
		type="numeric"
	    range="0:10000:110000"
		position="right"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:none;text-anchor:start"
		label_rotation="0"
		label_offset="12"
		line_style="stroke:none"
	/>
	
	<plot object="bar_plot1"
		id="plot2"
		type="hilo"
		values="L_min"
		values2="L_max"
		fill="#f0d373"
		stroke="#4d4d4d"
		style="stroke-width:0.5"
		bar_width="10"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="0"
		label_yoffset="-2"
	/>
	
	<plot object="xy_plot1"
		id="plot2"
		type="line"
		x_values=""
		y_values="L_mean"
		style="fill:none;stroke:#ffffff;stroke-opacity:0"
		marker="rechthoek"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="8"
		label_yoffset="-5"
	/>
	</g>
	<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
	
		     Hieronder volgt het statische deel van het sjabloon
	
	     ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->

	<!-- Scheiding tussen Plot1 en Plot2 -->
    <line x1= "349.5" y1="0.5" x2="349.5" y2="289.5" style="stroke:#ffffff;stroke-width:2.0"/>
    
    <line x1= "348" y1="0"   x2="351" y2="0"   style="stroke:#4d4d4d;stroke-width:0.5"/>
	<line x1= "348" y1="290" x2="351" y2="290" style="stroke:#4d4d4d;stroke-width:0.5"/>
	
	<!-- Tekst onder de grafiek -->
    <g transform="translate(0, 290)">
		<g style="stroke:#4d4d4d;stroke-width:0.5">
			<line x1= "10" y1="22" x2="340" y2="22"/>
			<line x1="360" y1="22" x2="690" y2="22"/>
		</g>
		<g style="font-family:Myriad Pro;font-size:14pt;font-weight:normal;fill:#333333;text-anchor:middle;">
			<text x="175" y="42">starts</text>
			<text x="525" y="42">landingen</text>
		</g>
	</g>
	
	<!-- Legenda -->
    <g transform="translate(530, -20)">
        <rect id="spreiding"  x="0"   y="-18" width="10.0" height="30.0" style="fill:#f0d373;stroke:#4d4d4d;stroke-width:0.5;"/>
        <rect id="gemiddelde" x="80"  y="-6"  width="30.0" height="5.0"  style="fill:#da9100;stroke:#4d4d4d;stroke-width:0.5;"/>

        <g style="font-family:Myriad Pro;font-size:11px;font-weight:normal;fill:#333333;text-anchor:left;">
            <text x="15"  y="0">spreiding</text>
            <text x="115" y="0">gemiddelde</text>
        </g>
    </g>

</g>

</svg>

