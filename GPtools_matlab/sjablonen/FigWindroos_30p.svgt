<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg viewBox="0 0 330 240"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">

<!-- Grafiek -->
<g transform="translate(50 20)">

    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot1"
		type="circle"
		radius="100"
		fill_style="fill:#d4dde8;fill-opacity:1.0"
		frame_style="stroke:#5badd5;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot1"
		type="polar"
		range="0:30:360"
		grid_style="stroke:#ffffff;stroke-width:0.5"
		decimals="0"
		decimal_sign=","
		label_postfix="%"
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:end"
		label_rotation="0"
		label_offset="-7"
		line_style="stroke:none"
	/>

	<plot object="y_axis"
		id="plot1"
		type="polar"
		angle="80"
		gap="10"
	    range="0:10:30"
		grid_style="stroke:#ffffff;stroke-width:0.5"
		decimals="0"
		decimal_sign=","
		label_postfix="%"
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:4pt;font-weight:normal;fill:#333333;text-anchor:end"
		label_rotation="-80"
		label_offset="0"
		line_style="stroke:none"
	/>
	
	<plot object="polar_bar_plot1"
		id="plot1"
		type="stacked"
		angles="winddir"
		values="percentage"
		fill="1,#c3defe,1;6,#566170,1"
		stroke="#3b6968"
		style="stroke-width:0.5"
		bar_width="2;3;5;7;9;11"
		decimals="0"
		decimal_sign=","
		label_postfix="%"
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:middle"
		label_xoffset="-17"
		label_yoffset="-8"
	/>
	
	<!-- gat in het midden -->
	<circle cx="100" cy="100" r="10" fill="#ffffff" stroke="#3b6968" stroke-width="0.5"  />
	
	<plot object="text"
		id="plot1"
		position="100,102"
		value="variable"
		format="%3.1f%%"
		style="font_family:Myriad Pro;font-size:4pt;font-weight:normal;fill:#3b6968;text-anchor:middle"
		rotation="0"
	/>
	
	<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
	
		     Hieronder volgt het statische deel van het sjabloon
	
	     ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
	
	<!-- labels -->
	<g style="font_family:Myriad Pro;font-size:10pt;font-weight:bold;fill:#333333;text-anchor:middle">
		<text x="100" y="-5">N</text>
		<text x="210" y="105">O</text>
		<text x="100" y="214">Z</text>
		<text x="-10"  y="105">W</text>
	</g>

<!-- 
	<g style="font_family:Myriad Pro;font-size:4pt;font-weight:normal;fill:#333333;text-anchor:end">
		<text x="139"   y="95">10%</text>
		<text x="168.5" y="90">20%</text>
		<text x="198"   y="85">30%</text>
	</g>
-->
		 
    <g transform="translate(205 125)">
		<!-- kader -->
		<rect width="70" height="87" x="0" y="0" style="fill:#d4dde8;fill-opacity:0.2;stroke:#5badd5;stroke-width:0.5" />

		<!-- Legenda -->
        <rect x="5"   y="05" width="11" height="10" style="fill:#566170;stroke:#3b6968;stroke-width:0.5;"/>
        <rect x="6"   y="15" width="9"  height="10" style="fill:#6C7A8C;stroke:#3b6968;stroke-width:0.5;"/>
        <rect x="7"   y="25" width="7"  height="10" style="fill:#8293A9;stroke:#3b6968;stroke-width:0.5;"/>
        <rect x="8"   y="35" width="5"  height="10" style="fill:#97ACC5;stroke:#3b6968;stroke-width:0.5;"/>
        <rect x="9"   y="45" width="3"  height="10" style="fill:#ADC5E2;stroke:#3b6968;stroke-width:0.5;"/>
        <rect x="9.5" y="55" width="2"  height="10" style="fill:#C3DEFE;stroke:#3b6968;stroke-width:0.5;"/>

		<circle cx="10.5" cy="70" r="5" fill="#ffffff" stroke="#3b6968" stroke-width="0.5"/>

        <g style="font-family:Myriad Pro;font-size:6pt;font-weight:normal;fill:#333333;text-anchor:end">
            <text x="65" y="15">&gt; 25 kts</text>
            <text x="65" y="25">20 - 25 kts</text>
            <text x="65" y="35">15 - 20 kts</text>
            <text x="65" y="45">10 - 15 kts</text>
            <text x="65" y="55">5 - 10 kts</text>
            <text x="65" y="65">&lt; 5 kts</text>
            <text x="65" y="75">variabel of</text>
            <text x="65" y="83">windstil</text>
        </g>
    </g>
</g>
</svg>
