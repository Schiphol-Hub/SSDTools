<!-- Let op het rekengebied van de noise heeft niet de verhouding van een A4 --> 
<svg width="210mm" height="240mm" <plot object="viewbox" id="ref" size="0,0,2100,2393"/>

     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">

<!-- Let op het rekengebied van de noise heeft niet de verhouding van een A4 --> 
<svg <plot object="viewbox" id="plot1" size="4000,16375,61000,69500"/>>
          
	<!--  Achtergrond: A4, 300 dpi -->
	<image xlink:href="Schiphol_RD900dpi.png" x="0" y="0" width="78750" height="111375"/>

	<g transform="scale(1,-1) translate(-80000,-541375)">
	
	<!-- Realisatie -->
	<plot object="contour_plot"
	    id="plot1"
		type="delta"
		grid="delta"
		dB="-10,-6:0.5:6,10"
		stroke="none"
		fill="-99,#00F600,0.9;-6,#00F600,0.9;-0.5,#E9FEC0,0.35;0,#FFFFFF,0.3;0.5,#FFF0C0,0.35;6,#FF6900,0.9;99,#FF6900,0.9"
		style="fill-rule:evenodd;stroke-width:5"
	/>

	<!-- betrouwbaarheidsinterval 48 en 58 dB contour -->
	<plot object="contour_plot"
		id="plot1a"
		type="hilo"
		grid="dhi"
		grid2="dlo"
		dB="48,58"
		stroke="48,#80b3ff,0.6;58,#0066ff,0.6"
		fill="48,#80b3ff,0.6;58,#0066ff,0.6"
		style="fill-rule:evenodd;stroke-width:50"
	/>

	<!-- nominale 48 en 58 dB contour -->
	<plot object="contour_plot"
		id="plot1b"
		type="line"
		grid="mean"
		dB="48,58"
		stroke="48,#80b3ff,0.6;58,#0066ff,0.6"
		fill="none"
		style="fill:none;stroke-width:150"
	/>
	<plot object="contour_plot"
	    id="plot1c"
		type="line"
		grid="mean"
		dB="48,58"
		stroke="48,#ffffff,1.0;58,#ffffff,1.0"
		fill="none"
		style="fill:none;stroke-width:100"
	/>
					
	</g>

</svg>


