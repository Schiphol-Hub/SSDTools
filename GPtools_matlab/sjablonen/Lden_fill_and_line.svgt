<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg width="210mm" height="297mm"
     <plot object="viewbox"
           id="ref"
           size="0,0,2100,2970"/>
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">

	 
<svg <plot object="viewbox" id="plot1" size="4000,5532,66000,93343"/>>
          
	<!--  Achtergrond: A4, 300 dpi -->
	<image xlink:href="Schiphol_RD900dpi.png" x="0" y="0" width="78750" height="111375"/>

	<g transform="scale(1,-1) translate(-80000,-541375)">
	
		<!-- Gevulde Lden contouren -->
		<plot object="contour_plot"
			id="plot1"
			type="flood"
			method="tape2"
			grid="mean"
			dB="46:0.1:48,49:70"
			stroke="46,#41789B,0.00;47.9,#4093A5,0.0;48,#4093A5,0.50;53,#5DBD52,0.50;58,#B3FF40,0.20;70,#ac3400,0.05"
			  fill="46,#41789B,0.05;47.9,#4093A5,0.3;48,#4093A5,0.30;53,#5DBD52,0.40;58,#B3FF40,0.50;70,#ac3400,0.50"
			style="fill-rule:evenodd;stroke-width:5"
		/>
		
		<!-- nominale 40 en 48 dB contour -->
		<plot object="contour_plot"
			id="plot1a"
			type="line"
			grid="mean"
			dB="48,58"
			stroke="48,#5fbcd3,1.0;58,#d35f5f,1.0"
			fill="none"
			style="fill:none;stroke-width:150"
		/>
	</g>

</svg>


<!-- Onderstaande staat op een vaste plaats op de pagina -->

<!-- schaalbalk -->
<g transform="translate(100,2750)">
	<plot object="scalebar"
		id="plot1"
		range="0:1000:10000"
		style1="fill:black;stroke:black;stroke-width:2"
		style2="fill:none;stroke:black;stroke-width:2"
	    height="10"
   		label="10 km"
		label_style="font-family:Myriad Pro;font-size:20pt;font-weight:normal;fill:#000000;text-anchor:start"
    />
</g>

<!-- Legenda -->
<g transform="translate(1690,2165)">
    <!-- kader -->
	<rect width="260" height="600" x="0" y="0" style="fill:#ffffff;fill-opacity:0.7;stroke:#aaaaaa;stroke-width:1.0" />

	<plot object="floodbar"
		id="plot1"
		width="75"
		height="600"
		range="46:70"
		style="stroke-width:0.5"
		tic_style="stroke:#000000;stroke-width:1.0"
		labels="48:5:68"
		label_style="font-family:Myriad Pro;font-size:20pt;font-weight:normal;fill:#000000;text-anchor:start"
	/>
</g>

</svg>