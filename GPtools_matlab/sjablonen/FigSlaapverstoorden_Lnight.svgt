<?xml version="1.0" encoding="iso-8859-1" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.0//EN" "http://www.w3.org/TR/SVG/DTD/svg10.dtd">
<svg viewBox="0 0 800 310"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink"
     xml:space="preserve">

<defs>
    <marker id="none"
      viewBox="0 0 10 10" refX="1" refY="1" 
      markerUnits="userSpaceOnUse"
      markerWidth="2" markerHeight="2"
      orient="0">
      <circle cx="1" cy="1" r="2"
        fill="none" stroke="none" stroke-width="1.0"  />
    </marker>

    <marker id="cirkel"
      viewBox="0 0 10 10" refX="5" refY="5" 
      markerUnits="userSpaceOnUse"
      markerWidth="8" markerHeight="8"
      orient="0">
      <circle cx="5" cy="5" r="4"
        fill="none" stroke="#666666" stroke-width="1.0"  />
    </marker>
    
    <marker id="streepje"
      viewBox="0 0 10 2" refX="4" refY="1" 
      markerUnits="userSpaceOnUse"
      markerWidth="6" markerHeight="1"
      orient="0">
      <rect width="8" height="2"
        fill="#666666" stroke="#666666" stroke-width="0.5"  />
    </marker>
    
    <marker id="streepje2"
      viewBox="0 0 10 2" refX="4" refY="1" 
      markerUnits="userSpaceOnUse"
      markerWidth="10" markerHeight="2"
      orient="0">
      <rect width="8" height="2"
        fill="#da9100" stroke="#da9100" stroke-width="0.5"  />
    </marker>
</defs>

     
<!-- Grafiek -->
<g transform="translate(50 50)">
	
    <!-- Plot opmaak voor Matlab -->
	<plot object="plot_area"
		id="plot1"
 		type="rectangle"
		width="700"
		height="210"
		fill_style="fill:#f8ebc7;fill-opacity:1.0"
		frame_style="stroke:#4d4d4d;stroke-width:0.5"
	/>

	<plot object="x_axis"
		id="plot1"
		type="numeric"
		range="2000:2018"
		values=""
		position="bottom"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator=""
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:middle"
		label_rotation="0"
		label_offset="12"
		line_style="stroke:none"
	/>
	
	<plot object="y_axis"
		id="plot1"
		type="numeric"
	    range="0:5000:30000"
		position="left"
		grid_style="stroke:#ffffff;stroke-width:1"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:end"
		label_rotation="0"
		label_offset="-7"
		line_style="stroke:none"
	/>
	
	<plot object="xy_plot1"
		id="plot1"
		type="area"
		x_values="fill_year"
		y_values="fill_sv40n_min"
		y_values2="fill_sv40n_max"
		style="stroke:none;fill:#e4af00;fill-opacity:0.2"
		marker="none"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="0"
		label_yoffset="0"
	/>
	
	<plot object="xy_plot2"
		id="plot1"
		type="normal"
		x_values="history_year"
		y_values="history_sv40n"
		style="stroke:#e4af00;stroke-width:1.5"
		marker="cirkel"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="0"
		label_yoffset="0"
	/>

	<plot object="xy_plot3"
		id="plot1"
		type="hilo"
		x_values="prognosis_year"
		y_values="prognosis_sv40n_min"
		y_values2="prognosis_sv40n_max"
		style="stroke:#666666;stroke-width:1.0"
		marker="streepje"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="3"
		label_yoffset="2"
	/>
	
	<plot object="xy_plot4"
		id="plot1"
		type="normal"
		x_values="prognosis_year"
		y_values="prognosis_sv40n_mean"
		style="fill:none;stroke:#ffffff;stroke-opacity:0"
		marker="streepje2"
		decimals="0"
		decimal_sign=","
		label_postfix=""
		thousands_seperator="."
		label_style="font_family:Myriad Pro;font-size:6pt;font-weight:normal;fill:none;text-anchor:start"
		label_xoffset="5"
		label_yoffset="2"
	/>

	<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
	
		     Hieronder volgt het statische deel van het sjabloon
	
	     ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
		 
    <!-- Legenda -->
    <g transform="translate(500, -20)">
		<polyline id="prog_minmax"     points="145,-6 145,14" style="stroke:#666666;stroke-width:1.0;marker:url(#streepje)" />
		<polyline id="prog_mean"       points="145,4"         style="stroke:#ffffff;stroke-opacity:0;stroke-width:1.5;marker:url(#streepje2)" />
		<polyline id="realisatie"      points="55,4 75,4"     style="stroke:#e4af00;stroke-width:1.5;marker:url(#cirkel)" />
		
        <g style="font-family:Myriad Pro;font-size:8pt;font-weight:normal;fill:#333333;text-anchor:left;">
            <text x="85"  y="6">realisatie</text>
            <text x="155" y="6">prognose</text>
        </g>
    </g>

</g>

</svg>

